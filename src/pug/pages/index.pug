extends ../layout/layout

include ../../blocks/carousel/carousel


block title
    - var titlePage = 'Title Page'
    title #{titlePage}

block config
    - var currentPage = 'currentPage'
    - var bodyClasses = 'bodyClasses'
    - var pageMainLayout = 'pageMainLayout'


block header
    +header({
        text: 'DIAMOND LAB',
        attr: 'DIAMOND LAB'
    })

block page
    div(class="page " + pageMainLayout )
        .container
            h2
                +icon('logo')
                | Variable
            ul
                li #{currentPage}
                li #{bodyClasses}
                li #{pageMainLayout}

            hr

            h2
                +icon('logo')
                | Animate Css Ready
            ul.animate-list
                each val in (['bounce', 'flash', 'pulse', 'rubberBand', 'shake', 'headShake', 'swing', 'tada', 'wobble', 'jello', 'bounceIn', 'bounceInDown', 'bounceInLeft', 'bounceInRight', 'bounceInUp','bounceOut','bounceOutDown','bounceOutLeft','bounceOutRight','bounceOutUp','fadeIn','fadeInDown','fadeInDownBig', 'fadeInLeft','fadeInLeftBig','fadeInRight','fadeInRightBig','fadeInUp','fadeInUpBig','fadeOut','fadeOutDown','fadeOutDownBig','fadeOutLeft','fadeOutLeftBig','fadeOutRight','fadeOutRightBig','fadeOutUp','fadeOutUpBig','flipInX','flipInY','flipOutX','flipOutY','lightSpeedIn','lightSpeedOut','rotateIn','rotateInDownLeft','rotateInDownRight','rotateInUpLeft','rotateInUpRight','rotateOut','rotateOutDownLeft','rotateOutDownRight','rotateOutUpLeft','rotateOutUpRight','hinge','rollIn','rollOut','zoomIn','zoomInDown','zoomInLeft','zoomInRight','zoomInUp','zoomOut','zoomOutDown','zoomOutLeft','zoomOutRight','zoomOutUp','slideInDown','slideInLeft','slideInRight','slideInUp','slideOutDown','slideOutLeft','slideOutRight','slideOutUp'])
                    li
                        +icon('logo')
                        | #{val}

            hr

            h2
                +icon('logo')
                | Carusel ready
            +carousel()

block footer
    +footer({
        text: 'diamond lab - startkit',
        src: ''
    })

