extends ../layout/layout


block title
    - var titlePage = 'Title Page'
    title #{titlePage}

block config
    - var currentPage = 'currentPage'
    - var bodyClasses = 'bodyClasses'
    - var pageMainLayout = 'pageMainLayout'


block header
    +header({
        text: 'Diamond Lab'
    })

block page
    div(class="page " + pageMainLayout)
        ul
            li #{currentPage}
            li #{bodyClasses}
            li #{pageMainLayout}
block footer
    +footer({
        text: 'diamond lab',
        src: ''
    })

