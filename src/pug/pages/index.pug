extends ../layout/layout

include ../../blocks/carousel/carousel


block title
    - var titlePage = 'Title Page'
    title #{titlePage}

block config
    - var currentPage = 'currentPage'
    - var bodyClasses = 'bodyClasses'
    - var pageMainLayout = 'pageMainLayout'


block header
    +header({
        text: 'Diamond Lab'
    })

    +icon('twitter-logo-silhouette')
    +svg('diamond')

block page
    div(class="page " + pageMainLayout )

        ul
            li #{currentPage}
            li #{bodyClasses}
            li #{pageMainLayout}

        hr
        +carousel()
        hr

block footer
    +footer({
        text: 'diamond lab',
        src: ''
    })

